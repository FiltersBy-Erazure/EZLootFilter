ItemDisplay[wms CLVL<23]:%YELLOW%I(%WHITE%Thaw%YELLOW%)I{%NL%%WHITE% +25% cold resistance for 30 seconds%NL%%WHITE%Clears %BLUE%frozen %WHITE%affliction and grants %NL% }// Thawing Potion

ItemDisplay[gpl CLVL<10]:%DARK_GREEN%+ %WHITE%Gas Potion %DARK_GREEN%+%NL%%WHITE%Strangling// Strangling Gas Potion
ItemDisplay[gpl CLVL>9]:{%WHITE% Strangling Potion }//
ItemDisplay[gpm CLVL<19]:%DARK_GREEN%+ %WHITE%Gas Potion %DARK_GREEN%+%NL%%WHITE%Choking// Choking Gas Potion
