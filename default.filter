// =========================================== Hide Inferior and Unnecessary Items =========================================== 

ItemDisplay[ear]:// Hide player ears
ItemDisplay[INF]:// Hide all Low Quality, Cracked, Crude, & Damaged items
ItemDisplay[aqv]:// Hides Arrow quivers
ItemDisplay[cqv]:// Hides Bolt quivers

// ================================================ Gold ===============================================

ItemDisplay[GOLD<10]:// Hide 10 gold and less at all times
ItemDisplay[GOLD<25 AND CLVL>4]:// Hide 25 gold and less if level 5+
ItemDisplay[GOLD<50 AND CLVL>7]:// Hide 50 gold and less if level 8+
ItemDisplay[GOLD<100 AND CLVL>10]:// Hide 100 gold and less if level 11+
ItemDisplay[GOLD<200 AND CLVL>14]:// Hide 200 gold and less if level 15+
ItemDisplay[GOLD<300 AND CLVL>21]:// Hide 300 gold and less if level 22+
ItemDisplay[GOLD<500 AND CLVL>28]:// Hide 500 gold and less if level 29+
ItemDisplay[GOLD<800 AND CLVL>35]:// Hide 800 gold and less if level 36+
ItemDisplay[GOLD<1400 AND CLVL>40]:// Hide 1400 gold and less if level 41+
ItemDisplay[GOLD<2000 AND CLVL>60]:// Hide 2000 gold and less if level 61+
ItemDisplay[GOLD<4000 AND CLVL>80]:// Hide 4000 gold and less if level 81+
ItemDisplay[GOLD<6500 AND CLVL>90]:// Hide 6500 gold and less if level 91+

// =========================================== Health Potions =========================================== 

// Hide lower quality potions based on character level
ItemDisplay[hp1 CLVL>12]:{%WHITE% Minor Healing Potion }//             	  Hides Minor Heal Potions after level 10
ItemDisplay[hp2 CLVL>20]:{%WHITE% Light Healing Potion }//                Hides Light Heal Potions after level 15
ItemDisplay[hp3 CLVL>24]:{%WHITE% Healing Potion }//                      Hides Heal Potions after level 24
ItemDisplay[hp4 CLVL>30]:{%WHITE% Greater Healing Potion }//              Hides Greater Heal Potions after level 30

ItemDisplay[hp5 CLVL>82]:{%WHITE% Super Healing Potion }//%RED%*I(%WHITE%H%RED%)I*//               Super Health Potions after level 84    		 *I(H)I* 1 line
ItemDisplay[hp5 (CLVL<83 AND CLVL>54)]:{%WHITE% Super Healing Potion }//%NL%%RED%*I(%WHITE%H%RED%)I*%NL%// between level 55 and 84  	  	  	 	 *I(H)I* 1 line
ItemDisplay[hp5 (CLVL<55 AND CLVL>24)]:{%WHITE% Super Healing Potion }//%NL%%RED%*I(%WHITE%H%RED%)I*%NL%// between level 25 and 54  	  	  	 	 *I(H)I* 1 line

// Default display for all Healing Potions *I(H)I*
ItemDisplay[hp1 OR hp2 OR hp3 OR hp4 OR hp5]:%RED%*I(%WHITE%H%RED%)I*//   Default display for all Healing Potions         	  	  				 *I(H)I* 1 line

// ======================================================= Mana Potions ======================================================= 

// Hide lower quality potions based on character level
ItemDisplay[mp1 CLVL>12]:{%WHITE% Minor Mana Potion }//                   Hides Minor Mana Potions after level 10
ItemDisplay[mp2 CLVL>20]:{%WHITE% Light Mana Potion }//                   Hides Light Mana Potions after level 15
ItemDisplay[mp3 CLVL>24]:{%WHITE% Mana Potion }//                         Hides Mana Potions after level 24
ItemDisplay[mp4 CLVL>35]:{%WHITE% Greater Mana Potion }//                 Hides Greater Mana Potions after level 35

// Adjust display of Super Mana Potions based on character level
ItemDisplay[mp5 CLVL>84]:{%WHITE% Super Mana Potion }%NL%%BLUE%*I(%WHITE%M%BLUE%)I*%NL%//              	Super Mana Potion after level 84  		 *I(M)I* 1 lines (add %NL% on both sides to change to 3 lines)
ItemDisplay[mp5 (CLVL<85 AND CLVL>64)]:{%WHITE% Super Mana Potion }%NL%%BLUE%*I(%WHITE%M%BLUE%)I*%NL%// between level 65 and 84   	  			 *I(M)I* 3 lines
ItemDisplay[mp5 (CLVL<65 AND CLVL>24)]:{%WHITE% Super Mana Potion }%NL%%BLUE%*I(%WHITE%M%BLUE%)I*%NL%// between level 25 and 64   	  			 *I(M)I* 3 lines

// Default display for all Mana Potions *I(M)I*
ItemDisplay[mp1 OR mp2 OR mp3 OR mp4 OR mp5]:%NL%%BLUE%*I(%WHITE%M%BLUE%)I*%NL%//   Default display for all Mana Potions  	  	  				 *I(M)I* 3 lines

// ======================================================= Rejuvenation Potions ======================================================= 

ItemDisplay[rvs]:%NL%%PURPLE%*I(%WHITE%Juv%PURPLE%)I*%NL%//            Rejuv Potion 			                         	 					*I(Juv)I* 3 lines
ItemDisplay[rvl]:%NL%%PURPLE%*I(%WHITE%Full%PURPLE%)I*%NL%//           Full Rejuv Potion 			                    	 					*I(Full)I* 3 lines

